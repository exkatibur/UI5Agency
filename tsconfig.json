{
  "compilerOptions": {
  "target": "es2023",
    "module": "es2022",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "allowJs": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "rootDir": "./",
    "baseUrl": "./",
    "paths": {
      "ui5/typescript/helloworld/*": ["./webapp/*"],
      "unit/*": ["./test/unit/*"],
      "integration/*": ["./test/integration/*"]
    },
    "composite": true
  },
  "include": ["./webapp/**/*", "./test/**/*", "./scripts/**/*", "./.github/agents/**/*"],
  "exclude": ["./test/**/*.qunit.ts", "./test/integration/**/*.ts"]
}
